<!--pages/user/index.wxml-->
<view class='avatar_name'>
  <image src='{{userInfo.avatarUrl}}'></image>
  <text>{{userInfo.nickName}}</text>
</view>

<view class='menu menu_1'>
  <FormIdNav my-class='menu_item'  bindtap="changeIndex">
    <image src='/img/icon-realname.png'></image>
    <view class='right_side'>
      <text>实名认证</text>
      <i-icon type="enter" size="22" color="#cbcbcb" />
    </view>
  </FormIdNav>
  <FormIdNav my-class='menu_item' url='/pages/gains/index?changeTitle=1'>
    <image src='/img/icon-userinfo.png'></image>
    <view class='right_side'>
      <text>今日战绩</text>
      <i-icon type="enter" size="22" color="#cbcbcb" />
    </view>
  </FormIdNav>
  
  <FormIdNav my-class='menu_item' url='/pages/gains/index?changeTitle=0'>
    <image src='/img/icon-myrecord.png'></image>
    <view class='right_side'>
      <text>本月战绩</text>
      <i-icon type="enter" size="22" color="#cbcbcb" />
    </view>
  </FormIdNav>
  <!-- <FormIdNav my-class='menu_item' url='/pages/income/index'>
